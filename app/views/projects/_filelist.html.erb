<table class="table table-hover .table-bordered">
	<thead>
		<tr>
			<th>文件</th>
			<!--
				<th>上传时间</th>
				<th>下载</th>
				-->
			</tr>
		</thead>
		<tbody>
			<% unless @projects.nil?%>
			<% @projects.each do |project| %>
			<tr>
				<td><a href="<%= project.file.url%>" class="link file_link"><%=File.basename project.file.url%></a></td>
			</tr>
			<% end %>
			<% end %>
		</tbody>
	</table>