<div class="container-fluid content-over">
  <div class="row-fluid">
    <div class="span12">

      <div class="row-fluid" id="container">
        <div class="span2">
          <%= render "projects/show_nav" %>
        </div>
        <div class="span10 well alpha">
  			<%= render "projects/show_content" %>
        </div>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
 
  $("div.module").fadeOut("fast");
  $("div#description").fadeIn();

  $("a.nav_button").click(function(event) {
    /* Act on the event */
    var id = $(this).attr("id");
    $("div.module").fadeOut("fast");
    $("div#"+id).fadeIn();
  });
  $("a#all").click(function(event) {
    /* Act on the event */
    $("div.module").fadeIn();
  });
</script>




